v0x14871f8b0_0 .var "push", 0 0;
v0x14871f960_0 .var "reset", 0 0;
S_0x148704fc0 .scope module, "uut" "fsm" 2 13, 3 1 0, S_0x148707590;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "coin";
    .port_info 3 /INPUT 1 "push";
    .port_info 4 /OUTPUT 1 "locked";
P_0x148707700 .param/l "LOCKED" 1 3 10, C4<0>;
P_0x148707740 .param/l "UNLOCKED" 1 3 11, C4<1>;
v0x148705130_0 .net "clk", 0 0, v0x14871f6c0_0;  1 drivers
v0x14871f2d0_0 .net "coin", 0 0, v0x14871f770_0;  1 drivers
v0x14871f370_0 .var "locked", 0 0;
v0x14871f420_0 .net "push", 0 0, v0x14871f8b0_0;  1 drivers
v0x14871f4c0_0 .net "reset", 0 0, v0x14871f960_0;  1 drivers
v0x14871f5a0_0 .var "state", 0 0;
E_0x1487058a0 .event anyedge, v0x14871f5a0_0;
E_0x148705a90 .event posedge, v0x14871f4c0_0, v0x148705130_0;
    .scope S_0x148704fc0;
T_0 ;
    %wait E_0x148705a90;
    %load/vec4 v0x14871f4c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x14871f5a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x14871f5a0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x14871f2d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.5, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x14871f5a0_0, 0;
T_0.5 ;
    %jmp T_0.4;
T_0.3 ;
    %load/vec4 v0x14871f420_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.7, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x14871f5a0_0, 0;
T_0.7 ;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x148704fc0;
T_1 ;
    %wait E_0x1487058a0;
    %load/vec4 v0x14871f5a0_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %store/vec4 v0x14871f370_0, 0, 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x148707590;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x14871f6c0_0;
    %inv;
    %store/vec4 v0x14871f6c0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x148707590;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f6c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14871f960_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f770_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f8b0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f960_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14871f770_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f770_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 32 "$display", "After coin inserted: locked = %b", v0x14871f800_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14871f8b0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f8b0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 37 "$display", "After push: locked = %b", v0x14871f800_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14871f770_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f770_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 42 "$display", "After second coin inserted: locked = %b", v0x14871f800_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14871f8b0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f8b0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 47 "$display", "After push to lock: locked = %b", v0x14871f800_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14871f770_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14871f770_0, 0, 1;
