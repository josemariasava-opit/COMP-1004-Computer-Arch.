:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x156006100 .scope module, "flip_flop_tb" "flip_flop_tb" 2 8;
 .timescale -9 -12;
v0x1560166d0_0 .var "D", 0 0;
v0x156016770_0 .net "Q", 0 0, v0x156016530_0;  1 drivers
v0x156016820_0 .var "clk", 0 0;
S_0x156006270 .scope module, "uut" "d_flip_flop" 2 14, 3 1 0, S_0x156006100;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "D";
    .port_info 2 /OUTPUT 1 "Q";
v0x156006480_0 .net "D", 0 0, v0x1560166d0_0;  1 drivers
v0x156016530_0 .var "Q", 0 0;
v0x1560165d0_0 .net "clk", 0 0, v0x156016820_0;  1 drivers
E_0x156005ff0 .event posedge, v0x1560165d0_0;
    .scope S_0x156006270;
T_0 ;
    %wait E_0x156005ff0;
    %load/vec4 v0x156006480_0;
    %assign/vec4 v0x156016530_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x156006100;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x156016820_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x156016820_0;
    %inv;
    %store/vec4 v0x156016820_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x156006100;
T_2 ;
    %vpi_call 2 28 "$dumpfile", "d_flip_flop.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x156006100 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1560166d0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1560166d0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1560166d0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_2;
