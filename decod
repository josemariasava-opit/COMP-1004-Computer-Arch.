E_0x600001db58c0 .event anyedge, v0x600003ab4090_0;
    .scope S_0x142606130;
T_0 ;
    %wait E_0x600001db58c0;
    %load/vec4 v0x600003ab4090_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
